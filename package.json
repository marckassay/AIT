{
  "name": "ait",
  "description": "An Ionic 4 application to target Android mobile devices",
  "version": "2.1.8",
  "private": true,
  "homepage": "https://github.com/marckassay/ait",
  "author": {
    "name": "Marc Kassay",
    "email": "marckassay@gmail.com",
    "url": "https://marckassay.github.io"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "wdio:test": "wdio ./e2e/appium/conf/wdio.conf.js",
    "wdio:standalone": "node ./e2e/appium/conf/wdio.standalone.js",
    "release:device": "ts-node -P ./scripts/tsconfig.scripts.json ./scripts/apk.ts --release --device",
    "debug:device": "ts-node -P ./scripts/tsconfig.scripts.json ./scripts/apk.ts --device",
    "fix": "node plugins/cordova-plugin-lottie-splashscreen/hooks/android/copy_build_extras.js",
    "kill": "adb kill-server"
  },
  "dependencies": {
    "@angular/common": "^7.2.14",
    "@angular/core": "^7.2.14",
    "@angular/forms": "^7.2.14",
    "@angular/http": "^7.2.14",
    "@angular/platform-browser": "^7.2.14",
    "@angular/platform-browser-dynamic": "^7.2.14",
    "@angular/router": "^7.2.14",
    "@ionic-native/android-full-screen": "^5.5.1",
    "@ionic-native/audio-management": "^5.5.1",
    "@ionic-native/brightness": "^5.5.1",
    "@ionic-native/core": "^5.5.1",
    "@ionic-native/lottie-splash-screen": "^5.5.1",
    "@ionic-native/native-audio": "^5.5.1",
    "@ionic-native/screen-orientation": "^5.5.1",
    "@ionic-native/status-bar": "^5.5.1",
    "@ionic-native/vibration": "^5.5.1",
    "@ionic/angular": "^4.3.1",
    "@ionic/storage": "^2.2.0",
    "clovelced-plugin-audiomanagement": "^1.0.2",
    "cordova-android": "8.0.0",
    "cordova-plugin-brightness": "^0.2.0",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-fullscreen": "1.2.0",
    "cordova-plugin-ionic-webview": "^3.1.2",
    "cordova-plugin-lottie-splashscreen": "0.5.0",
    "cordova-plugin-nativeaudio": "^3.0.9",
    "cordova-plugin-screen-orientation": "^3.0.1",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-vibration": "^3.1.0",
    "cordova-plugin-whitelist": "^1.3.3",
    "core-js": "^2.5.4",
    "es6-promise-plugin": "^4.2.2",
    "moment": "^2.24.0",
    "rxjs": "^6.4.0",
    "sots": "^4.0.4",
    "zone.js": "~0.8.29"
  },
